{
  "name": "eslint-plugin-error",
  "version": "1.2.0",
  "description": "ESLint plugin with rules for JavaScript and TypeScript error handling",
  "type": "module",
  "main": "src/index.js",
  "files": [
    "src"
  ],
  "scripts": {
    "test": "vitest --run",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .js",
    "lint:fix": "eslint src --ext .js --fix",
    "validate": "pnpm test && pnpm lint",
    "release": "dotenv -e .env -- release-it",
    "release:dry": "dotenv -e .env -- release-it --dry-run",
    "publish:ci": "dotenv -e .env -- release-it --ci"
  },
  "keywords": [
    "eslint",
    "eslint-plugin",
    "error-handling",
    "typescript",
    "javascript"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tupe12334/eslint-plugin-error.git"
  },
  "bugs": {
    "url": "https://github.com/tupe12334/eslint-plugin-error/issues"
  },
  "homepage": "https://github.com/tupe12334/eslint-plugin-error#readme",
  "devDependencies": {
    "@release-it/conventional-changelog": "^10.0.1",
    "@vitest/coverage-v8": "^1.6.0",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9.0.0",
    "release-it": "^19.0.5",
    "vitest": "^1.6.0"
  },
  "peerDependencies": {
    "eslint": "^9.0.0"
  },
  "engines": {
    "node": ">=18"
  }
}
